---
title:      Star Deal
desc:       Default daily Star Deal template
region:     London
culture:    en-GB
layout:     bookatable-default
---

<!-- divider -->
{{> divider }}

<!-- STAR DEAL STRAPLINE -->
{{> stardeal-strapline }}
<!-- STAR DEAL STRAPLINE -->

<!-- divider -->
{{> divider }}

<!-- STAR DEAL HEADER TEXT -->
<table class="row theme-cerulean">
    <tr>
        <td class="wrapper">
            <table class="eight columns">
                <tr>
                    <td>{{> stardeal-header-text}}</td>
                    <td class="expander"></td>
                </tr>
            </table>
        </td>
        <td class="wrapper last">
            <table class="four columns">
                <tr>
                    <td>{{> stardeal-header-button}}</td>
                    <td class="expander"></td>
                </tr>
            </table>
        </td>
    </tr>
</table>
<!-- STAR DEAL HEADER TEXT -->

<!-- STAR DEAL HERO -->
<table class="row">
    <tr>
        <td class="wrapper last">
            <table class="twelve columns">
                <tr>
                    <td>{{> stardeal-hero}}</td>
                    <td class="expander"></td>
                </tr>
            </table>
        </td>
    </tr>
</table>
<!-- STAR DEAL HERO -->

<!-- divider -->
{{> divider }}

<!-- STAR DEAL CONTENT -->
<table class="row">
    <tr>
        <td class="wrapper">

            <table class="six columns">
                <tr>
                    <td class="theme-white">{{> stardeal-desc }}</td>
                    <td class="expander"></td>
                </tr>
                <tr>
                	<td class="theme-white">{{> stardeal-buttons }}</td>
                	<td class="expander"></td>
                </tr>
                
                <tr>
                	<td class="theme-white">{{> stardeal-review }}</td>
                	<td class="expander"></td>
                </tr>
                
                <tr>
                	<td class="theme-white">{{> stardeal-quote }}</td>
                	<td class="expander"></td>
                </tr>
            </table>

        </td>
        <td class="wrapper last">

            <table class="six columns">
                <tr>
                    <td class="theme-white">

                        {{> stardeal-menu }}


                    </td>
                    <td class="expander"></td>
                </tr>
            </table>

        </td>
    </tr>
</table>
<!-- STAR DEAL CONTENT -->

<!-- STAR DEAL TERMS -->
<table class="row">
    <tr>
        <td class="wrapper last">
            <table class="twelve columns">
                <tr>
                    <td>
                        {{> stardeal-terms }}
                    </td>
                    <td class="expander"></td>
                </tr>
            </table>
        </td>
    </tr>
</table>
<!-- STAR DEAL TERMS -->

{{> divider }}


<!-- VIEW ALL BANNER -->

<table class="row">
    <tr>
        <td class="wrapper last">
            <table class="twelve columns">
                <tr>
                    <td class="center">
                       <center>                        
                       	{{> stardeal-banner }}
                       </center>
                    </td>
                    <td class="expander"></td>
                </tr>
            </table>
        </td>
    </tr>
</table>

<!-- VIEW ALL BANNER -->

<!-- divider -->
{{> divider }}

<!-- PREFERENCE -->
<table class="row">
    <tr>
        <td class="wrapper last">
            <table class="twelve columns">
                <tr>
                    <td class="theme-white">{{> stardeal-pref }}</td>
                    <td class="expander"></td>
                </tr>
            </table>
        </td>
    </tr>
</table>
<!-- PREFERENCE -->

<!-- divider -->
{{> divider }}
<!-- divider -->